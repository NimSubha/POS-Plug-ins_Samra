          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SubTotalTableAdapter" GeneratorDataComponentClassName="SubTotalTableAdapter" Name="SubTotal" UserDataComponentName="SubTotalTableAdapter">
