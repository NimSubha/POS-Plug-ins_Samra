                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP 1 CAST(RATES AS decimal (6,2)) AS STDGOLDRATE,
(SELECT DEFAULTCONFIGIDGOLD FROM RETAILPARAMETERS WHERE DATAAREAID = @DATAAREAID) AS BASECONFIG 
FROM METALRATES
WHERE INVENTLOCATIONID= @INVENTLOCATIONID
AND METALTYPE=1
AND RETAIL=1 AND RATETYPE = 3
AND ACTIVE=1 
